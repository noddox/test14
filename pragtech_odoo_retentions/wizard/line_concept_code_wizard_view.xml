<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="insert_concept_code_wizard_view_form" model="ir.ui.view">
        <field name="name">line.concept.code.wizard.form</field>
        <field name="model">line.concept.code.wizard</field>
        <field name="arch" type="xml">
            <form string="Add Cocept Code">
                <group>

                    <field name="concept_code"/>
                </group>

                <footer>
                    <button name="add_concept_code" string="Add" type="object" class="btn-primary"/>

                    <button string="Close" class="btn-primary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>



</odoo>
